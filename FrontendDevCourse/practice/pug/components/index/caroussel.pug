        .caroussel-item__details
          .caroussel-item__details--buttons
            img(src="https://img.icons8.com/ios-glyphs/30/000000/circled-play.png" alt="Reproducir")
            img(src="https://img.icons8.com/ios-glyphs/120/000000/plus.png" alt="Añadir a lista")
          p.caroussel-item__details--title Título descriptivo
          p.caroussel-item__details--subtitle 2019 16+ 114 minutos
        